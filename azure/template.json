    },
    "version": {
      "type": "string",
      "defaultValue": "1.0"
    },
    "appServicePlanInstances": {
      "type": "int",
      "defaultValue": 2
    },
    "loggingRedisConnectionString": {
      "type": "securestring"
    },
    "functionsExtensionVersion": {
